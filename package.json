{
    "name": "yolkbot",
    "description": "yolkbot builds your dream shell shockers bot.",
    "version": "1.5.2",
    "keywords": [
        "shell shockers",
        "league bot",
        "blue wizard",
        "yolkbot"
    ],
    "homepage": "https://yolkbot.xyz",
    "bugs": {
        "url": "https://github.com/yolkorg/yolkbot/issues",
        "email": "hello@villainsrule.xyz"
    },
    "author": "1ust",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yolkorg/yolkbot.git"
    },
    "type": "module",
    "license": "SEE LICENSE IN LICENSE.md",
    "files": [
        "browser/build/*.js",
        "dist/**/*",
        "package.json",
        "README.md",
        "LICENSE.md"
    ],
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./api": {
            "import": "./dist/api.js",
            "types": "./dist/api.d.ts"
        },
        "./bot": {
            "import": "./dist/bot.js",
            "types": "./dist/bot.d.ts"
        },
        "./enums": {
            "import": "./dist/enums.js",
            "types": "./dist/enums.d.ts"
        },
        "./socket": {
            "import": "./dist/socket.js",
            "types": "./dist/socket.d.ts"
        },
        "./util": {
            "import": "./dist/util.js",
            "types": "./dist/util.d.ts"
        },
        "./browser": "./browser/build/module.js",
        "./browser/*": "./browser/build/*.js",
        "./browser/*.js": "./browser/build/*.js",
        "./bot/*": {
            "import": "./dist/bot/*.js",
            "types": "./dist/bot/*.d.ts"
        },
        "./bot/*.js": {
            "import": "./dist/bot/*.js",
            "types": "./dist/bot/*.d.ts"
        },
        "./comm/*": {
            "import": "./dist/comm/*.js",
            "types": "./dist/comm/*.d.ts"
        },
        "./comm/*.js": {
            "import": "./dist/comm/*.js",
            "types": "./dist/comm/*.d.ts"
        },
        "./constants/*": {
            "import": "./dist/constants/*.js",
            "types": "./dist/constants/*.d.ts"
        },
        "./constants/*.js": {
            "import": "./dist/constants/*.js",
            "types": "./dist/constants/*.d.ts"
        },
        "./dispatch": {
            "import": "./dist/dispatches/index.js",
            "types": "./dist/dispatches.d.ts"
        },
        "./dispatch/*": {
            "import": "./dist/dispatches/*.js",
            "types": "./dist/dispatches/*.d.ts"
        },
        "./dispatch/*.js": {
            "import": "./dist/dispatches/*.js",
            "types": "./dist/dispatches/*.d.ts"
        },
        "./dispatches": {
            "import": "./dist/dispatches/index.js",
            "types": "./dist/dispatches.d.ts"
        },
        "./dispatches/*": {
            "import": "./dist/dispatches/*.js",
            "types": "./dist/dispatches/*.d.ts"
        },
        "./dispatches/*.js": {
            "import": "./dist/dispatches/*.js",
            "types": "./dist/dispatches/*.d.ts"
        },
        "./pathing/*": {
            "import": "./dist/pathing/*.js",
            "types": "./dist/pathing/*.d.ts"
        },
        "./pathing/*.js": {
            "import": "./dist/pathing/*.js",
            "types": "./dist/pathing/*.d.ts"
        },
        "./wasm": {
            "import": "./dist/wasm/direct.js",
            "types": "./dist/wasm/direct.d.ts"
        },
        "./wasm/*": {
            "import": "./dist/wasm/*.js",
            "types": "./dist/wasm/*.d.ts"
        },
        "./wasm/*.js": {
            "import": "./dist/wasm/*.js",
            "types": "./dist/wasm/*.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            ".": ["dist/index.d.ts"],
            "api": ["dist/api.d.ts"],
            "bot": ["dist/bot/*.d.ts"],
            "enums": ["dist/enums.d.ts"],
            "socket": ["dist/socket.d.ts"],
            "util": ["dist/util.d.ts"],
            "bot/*": ["dist/bot/*.d.ts"],
            "comm/*": ["dist/comm/*.d.ts"],
            "constants/*": ["dist/constants/*.d.ts"],
            "dispatch": ["dist/dispatches.d.ts"],
            "dispatch/*": ["dist/dispatches/*.d.ts"],
            "dispatches": ["dist/dispatches.d.ts"],
            "dispatches/*": ["dist/dispatches/*.d.ts"],
            "pathing/*": ["dist/pathing/*.d.ts"],
            "wasm": ["dist/wasm/direct.d.ts"],
            "wasm/*": ["dist/wasm/*.d.ts"]
        }
    },
    "dependencies": {
        "wwws": "^1.1.5"
    },
    "devDependencies": {
        "@stylistic/eslint-plugin": "^4.4.1",
        "@types/bun": "^1.3.5",
        "eslint": "^9.39.2",
        "tsx": "^4.21.0"
    }
}