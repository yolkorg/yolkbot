{
    "name": "yolkbot",
    "description": "create a shell shockers bot in under 10 lines.",
    "version": "1.4.6",
    "keywords": [
        "shell shockers",
        "shellshock.io",
        "blue wizard",
        "yolkbot"
    ],
    "homepage": "https://yolkbot.xyz",
    "bugs": {
        "url": "https://github.com/yolkorg/yolkbot/issues",
        "email": "bugs@villainsrule.xyz"
    },
    "author": "1ust",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yolkorg/yolkbot.git"
    },
    "type": "module",
    "files": [
        "browser/build/*.js",
        "dist/**/*",
        "package.json",
        "README.md"
    ],
    "exports": {
        "./package.json": "./package.json",
        ".": "./dist/index.js",
        "./api": {
            "import": "./dist/api.js",
            "types": "./dist/api.d.ts"
        },
        "./browser": "./browser/build/module.js",
        "./browser/*": "./browser/build/*.js",
        "./browser/*.js": "./browser/build/*.js",
        "./comm": {
            "import": "./dist/comm/index.js",
            "types": "./dist/comm/index.d.ts"
        },
        "./matchmaker": {
            "import": "./dist/matchmaker.js",
            "types": "./dist/matchmaker.d.ts"
        },
        "./util": {
            "import": "./dist/util.js",
            "types": "./dist/util.d.ts"
        },
        "./pathing/*": "./dist/pathing/*.js",
        "./pathing/*.js": "./dist/pathing/*.js",
        "./bot": {
            "import": "./dist/bot.js",
            "types": "./dist/bot.d.ts"
        },
        "./bot/*": {
            "import": "./dist/bot/*.js",
            "types": "./dist/bot/*.d.ts"
        },
        "./bot/*.js": {
            "import": "./dist/bot/*.js",
            "types": "./dist/bot/*.d.ts"
        },
        "./constants": {
            "import": "./dist/constants/index.js",
            "types": "./dist/constants/index.d.ts"
        },
        "./constants/*": {
            "import": "./dist/constants/*.js",
            "types": "./dist/constants/*.d.ts"
        },
        "./constants/*.js": {
            "import": "./dist/constants/*.js",
            "types": "./dist/constants/*.d.ts"
        },
        "./dispatch": {
            "import": "./dist/dispatches/index.js",
            "types": "./dist/dispatches.d.ts"
        },
        "./dispatch/*": {
            "import": "./dist/dispatches/*.js",
            "types": "./dist/dispatches/*.d.ts"
        },
        "./dispatch/*.js": {
            "import": "./dist/dispatches/*.js",
            "types": "./dist/dispatches/*.d.ts"
        },
        "./dispatches": {
            "import": "./dist/dispatches/index.js",
            "types": "./dist/dispatches.d.ts"
        },
        "./dispatches/*": {
            "import": "./dist/dispatches/*.js",
            "types": "./dist/dispatches/*.d.ts"
        },
        "./dispatches/*.js": {
            "import": "./dist/dispatches/*.js",
            "types": "./dist/dispatches/*.d.ts"
        },
        "./socket": {
            "import": "./dist/socket.js",
            "types": "./dist/socket.d.ts"
        },
        "./wasm": {
            "import": "./dist/wasm/direct.js",
            "types": "./dist/wasm/direct.d.ts"
        },
        "./wasm/*": {
            "import": "./dist/wasm/*.js",
            "types": "./dist/wasm/*.d.ts"
        },
        "./wasm/*.js": {
            "import": "./dist/wasm/*.js",
            "types": "./dist/wasm/*.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            "api": ["dist/api.d.ts"],
            "comm": ["dist/comm/index.d.ts"],
            "matchmaker": ["dist/matchmaker.d.ts"],
            "util": ["dist/util.d.ts"],
            "bot": ["dist/bot/*.d.ts"],
            "bot/*": ["dist/bot/*.d.ts"],
            "constants": ["dist/constants/*.d.ts"],
            "constants/*": ["dist/constants/*.d.ts"],
            "dispatch": ["dist/dispatches.d.ts"],
            "dispatch/*": ["dist/dispatches/*.d.ts"],
            "dispatches": ["dist/dispatches.d.ts"],
            "dispatches/*": ["dist/dispatches/*.d.ts"],
            "wasm": ["dist/wasm/direct.d.ts"],
            "wasm/*": ["dist/wasm/*.d.ts"]
        }
    },
    "dependencies": {
        "wwws": "^1.1.4"
    },
    "devDependencies": {
        "@stylistic/eslint-plugin": "^4.4.1",
        "@types/bun": "^1.3.5",
        "eslint": "^9.39.2"
    },
    "scripts": {
        "lint": "bunx --bun eslint . --fix"
    }
}